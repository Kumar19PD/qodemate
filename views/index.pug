doctype html
html

head
link(rel='stylesheet' type='text/css' href='md-icons/dist/material-design-icons.css')
link(rel='stylesheet' type='text/css' href='bootstrap/dist/css/bootstrap.min.css')
link(rel='stylesheet' type='text/css' href='style.css')
title= title

body.qm
nav.navbar.fixed-bottom
	a.navbar-brand(href='/')
		img(src='img/qm_logo_Q.png' height='24')
	ul.nav.justify-content-center
		li.nav-item: a.nav-link.qm(href='/helium'): i.material-icons.md-light picture_in_picture
		li.nav-item: a.nav-link.qm(href='/previous'): i.material-icons.md-light navigate_before
		li.nav-item: a.nav-link.qm(href='/play'): i.material-icons.md-light play_arrow
		li.nav-item: a.nav-link.qm(href='/next'): i.material-icons.md-light navigate_next
		li.nav-item: a.nav-link.qm(href='/reset'): i.material-icons.md-light autorenew
	ul.nav.justify-content-right
		li.nav-item: a.nav-link.qm(href='/exit'): i.material-icons.md-light close
		
div.container.row.col-12.m-0.p-0
	div.col-12
		div(id='player')
		p!= md
script(src='jquery/dist/jquery.min.js')
script(src='moment/min/moment.min.js')
script(src='tether/dist/js/tether.min.js')
script(src='popper/dist/popper.min.js')
script(src='bootstrap/dist/js/bootstrap.min.js')
script(src='p5/lib/p5.min.js')
script(src='sketch.js')
